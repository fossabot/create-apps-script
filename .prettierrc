{
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "parser": "babel-flow",
  "proseWrap": "always",
  "trailingComma": "none",
  "printWidth": 80,
  "tabWidth": 2,
  "bracketSpacing": false,
  "insertPragma": true,
  "requirePragma": false,
  "semi": false,
  "singleQuote": true,
  "useTabs": false
}
